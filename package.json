{
  "name": "@aixpand/client",
  "version": "0.29.5",
  "description": "This is the Typescript client for the AiXpand network.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "bin": {
    "aixpand": "./dist/aixpand.cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:AiXpand/tsclient.git"
  },
  "keywords": [
    "AiXpand",
    "Typescript Client",
    "SDK"
  ],
  "author": "AiXpand Team",
  "contributors": [
    "Radu Lupaescu <radu.lupaescu@gmail.com>",
    "Narcis Cruceru <narcis.cru@gmail.com>",
    "Paul Purcel <sarrieri@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AiXpand/tsclient/issues"
  },
  "homepage": "https://github.com/AiXpand/tsclient#readme",
  "dependencies": {
    "@types/uuid": "^9.0.2",
    "asn1.js": "^5.4.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "elliptic": "^6.5.4",
    "eventemitter2": "^6.4.9",
    "js-yaml": "^4.1.0",
    "json-stable-stringify": "^1.0.2",
    "mqtt": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typescript": "^5.1.6",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/json-stable-stringify": "^1.0.34",
    "@types/node": "^20.5.4",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "eslint": "^8.47.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.2",
    "ts-jest": "^29.1.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "./src/**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
